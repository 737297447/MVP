<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle" >

    <!-- 设置边框的大小和颜色 -红色1dp边框的 -->
    <stroke
        android:width="1dp"
        android:color="#f9f6f1" />

    <!-- 设置图形内的颜色,此处为红色 -->

    <solid android:color="#99000000" />

    <!-- 定义圆角弧度 圆的话就是半径 -->
    <corners android:radius="5dp" />

</shape>